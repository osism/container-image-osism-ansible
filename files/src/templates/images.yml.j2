---
# DO NOT EDIT THIS FILE BY HAND -- YOUR CHANGES WILL BE OVERWRITTEN

adminer_tag: "{{ '{{' }} repository_version {{ '}}' }}"
adminer_image: "{{ '{{' }} docker_registry {{ '}}' }}/osism/adminer:{{ '{{' }} adminer_tag {{ '}}' }}"

aptly_tag: "{{ '{{' }} repository_version {{ '}}' }}"
aptly_image: "{{ '{{' }} docker_registry {{ '}}' }}/osism/aptly:{{ '{{' }} aptly_tag {{ '}}' }}"

ara_server_tag: "{{ '{{' }} repository_version {{ '}}' }}"
ara_server_image: "{{ '{{' }} docker_registry {{ '}}' }}/osism/ara-server:{{ '{{' }} ara_server_tag {{ '}}' }}"

cobbler_tag: "{{ '{{' }} repository_version {{ '}}' }}"
cobbler_image: "{{ '{{' }} docker_registry {{ '}}' }}/osism/cobbler:{{ '{{' }} cobbler_tag {{ '}}' }}"

nginx_tag: "{{ '{{' }} repository_version {{ '}}' }}"
nginx_image: "{{ '{{' }} docker_registry {{ '}}' }}/osism/nginx:{{ '{{' }} nginx_tag {{ '}}' }}"

phpmyadmin_tag: "{{ '{{' }} repository_version {{ '}}' }}"
phpmyadmin_image: "{{ '{{' }} docker_registry {{ '}}' }}/osism/phpmyadmin:{{ '{{' }} phpmyadmin_tag {{ '}}' }}"

registry_tag: "{{ '{{' }} repository_version {{ '}}' }}"
registry_image: "{{ '{{' }} docker_registry {{ '}}' }}/osism/registry:{{ '{{' }} registry_tag {{ '}}' }}"

# image tag depends on CEPH_VERSION or OPENSTACK_VERSION

ceph_ansible_tag: "{{ '{{' }} ceph_version {{ '}}' }}-{{ '{{' }} manager_version {{ '}}' }}"
ceph_ansible_image: "{{ '{{' }} docker_registry {{ '}}' }}/osism/ceph-ansible:{{ '{{' }} ceph_ansible_tag {{ '}}' }}"

cephclient_tag: "{{ '{{' }} ceph_version {{ '}}' }}"
cephclient_image: "{{ '{{' }} docker_registry {{ '}}' }}/osism/cephclient:{{ '{{' }} cephclient_tag {{ '}}' }}"

kolla_ansible_tag: "{{ '{{' }} openstack_version {{ '}}' }}-{{ '{{' }} manager_version {{ '}}' }}"
kolla_ansible_image: "{{ '{{' }} docker_registry {{ '}}' }}/osism/kolla-ansible:{{ '{{' }} kolla_ansible_tag {{ '}}' }}"

openstackclient_tag: "{{ '{{' }} openstack_version {{ '}}' }}"
openstackclient_image: "{{ '{{' }} docker_registry {{ '}}' }}/osism/openstackclient:{{ '{{' }} openstackclient_tag {{ '}}' }}"

osism_ansible_tag: "{{ '{{' }} manager_version {{ '}}' }}"
osism_ansible_image: "{{ '{{' }} docker_registry {{ '}}' }}/osism/osism-ansible:{{ '{{' }} osism_ansible_tag {{ '}}' }}"

# re-used images from kolla

rally_tag: "{{ '{{' }} openstack_version {{ '}}' }}-{{ '{{' }} repository_version {{ '}}' }}"
rally_image: "{{ '{{' }} docker_registry {{ '}}' }}/osism/rally:{{ '{{' }} rally_tag {{ '}}' }}"

# images with OPENSTACK_VERSION-REPOSITORY_VERSION and REPOSITORY_VERSION tags

mariadb_tag: "{{ '{{' }} repository_version {{ '}}' }}"
mariadb_image: "{{ '{{' }} docker_registry {{ '}}' }}/osism/mariadb:{{ '{{' }} mariadb_tag {{ '}}' }}"

redis_tag: "{{ '{{' }} repository_version {{ '}}' }}"
redis_image: "{{ '{{' }} docker_registry {{ '}}' }}/osism/redis:{{ '{{' }} redis_tag {{ '}}' }}"
