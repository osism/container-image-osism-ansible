---
# DO NOT EDIT THIS FILE BY HAND -- YOUR CHANGES WILL BE OVERWRITTEN

roles:
{% for rolename in ansible_role_names %}
  - name: {{ rolename }}
    src: git+https://github.com/{{ ansible_role_names[rolename] }}.git
    version: {{ ansible_roles[rolename] }}
{% endfor %}

collections:
  - name: ansible.netcommon
    source: https://galaxy.ansible.com

  - name: ansible.posix
    source: https://galaxy.ansible.com

  - name: arista.cvp
    source: https://galaxy.ansible.com

  - name: arista.eos
    source: https://galaxy.ansible.com

  - name: awx.awx
    source: https://galaxy.ansible.com

  - name: community.general
    source: https://galaxy.ansible.com

  - name: community.network
    source: https://galaxy.ansible.com

  # NOTE: Use Git for community.zabbix for now until a release > 1.1.0 happens
  #       Necessary because with Zabbix 5.2 templateScreens is no longer usable
  #       and this detection is done only in > 1.1.0

  # - name: community.zabbix
  #   source: https://galaxy.ansible.com

  - name: https://github.com/ansible-collections/community.zabbix.git
    type: git

  - name: debops.debops
    version: 2.0.3
    source: https://galaxy.ansible.com

  - name: netbox.netbox
    source: https://galaxy.ansible.com

  - name: openstack.cloud
    source: https://galaxy.ansible.com

  - name: https://github.com/osism/ansible-collection-commons.git
    type: git

  - name: https://github.com/osism/ansible-collection-services.git
    type: git

  - name: https://github.com/osism/ansible-collection-validations.git
    type: git
